PROCEDURE "NEW_RETAILER_ID"(
    OUT OUT_RETAILER_ID NVARCHAR(15)

  ) 
  LANGUAGE SQLSCRIPT 
  SQL SECURITY INVOKER 
  --DEFAULT SCHEMA <default_schema_name>
  AS 
BEGIN
--   DECLARE LV_CREATION_DATE DATE;
--   DECLARE LV_PDC_ID BIGINT;
  DECLARE LV_RETAILER_ID NVARCHAR(15);

  SELECT "RETAILER_ID".NEXTVAL INTO LV_RETAILER_ID FROM DUMMY;
  OUT_RETAILER_ID := :LV_RETAILER_ID;
  
END
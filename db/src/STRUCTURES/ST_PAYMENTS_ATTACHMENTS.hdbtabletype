TYPE "ST_PAYMENTS_ATTACHMENTS" AS TABLE(
  "POP_NO" BIGINT NOT NULL,
  "ATTACH_CODE" INTEGER,
  "FILE_ID" INTEGER NOT NULL,
  "FILE_CONTENT" BLOB,
  "FILE_MIMETYPE" NVARCHAR(100),
  "FILE_TYPE" NVARCHAR(100),
  "FILE_NAME" NVARCHAR(100),
  "CREATION_DATE" TIMESTAMP,
  PRIMARY KEY("POP_NO", "FILE_ID")
)
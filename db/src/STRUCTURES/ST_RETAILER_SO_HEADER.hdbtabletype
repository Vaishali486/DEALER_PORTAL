TYPE "ST_RETAILER_SO_HEADER" AS TABLE(
  
    "DISTRIBUTOR_ID" NVARCHAR(10),
    "SO_NO" NVARCHAR(10),
    "RETAILER_ID"  NVARCHAR(10),
    "RETAILER_NAME" NVARCHAR(50),
    "CREATION_DATE" DATE,
    "DIVISION" NVARCHAR(5),
    "SHIP_TO_PARTY" NVARCHAR(600),
    "GROSS_TOTAL" NVARCHAR(100),
    "STATUS" INTEGER,
    
    PRIMARY KEY("DISTRIBUTOR_ID","SO_NO", "RETAILER_ID")

)
TYPE "ST_RETAILER_PAYMENT_EVENT_LOGS" as TABLE (

  SR_NO INTEGER NOT NULL,
  SO_NO NVARCHAR(10) NOT NULL,
  INVOICE_NO NVARCHAR(15) NOT NULL,
  DELIVERY_NO NVARCHAR(10) NOT NULL,
  USER_ID NVARCHAR(100),
  USER_NAME NVARCHAR(50),
  USER_ROLE NVARCHAR(50),
  COMMENT NVARCHAR(200),
  CREATED_ON TIMESTAMP,
  PRIMARY KEY(SR_NO, SO_NO, INVOICE_NO, DELIVERY_NO) 
)

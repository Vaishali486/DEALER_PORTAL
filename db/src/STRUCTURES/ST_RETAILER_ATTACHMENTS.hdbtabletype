TYPE "ST_RETAILER_ATTACHMENTS" AS TABLE(
    "DISTRIBUTOR_ID" NVARCHAR(10),
    "RETAILER_ID" NVARCHAR(10),
    "FILE_ID" INTEGER,
    "FILE_CONTENT" BLOB,
    "FILE_MIMETYPE" NVARCHAR(100),
    "FILE_TYPE" NVARCHAR(100),
    "FILE_NAME" NVARCHAR(100),
    PRIMARY KEY("DISTRIBUTOR_ID", "RETAILER_ID")

)
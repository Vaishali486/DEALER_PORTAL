TYPE "ST_PROMOTERS" AS TABLE(
    "REQUEST_NO" BIGINT NOT NULL,
    "SR_NO" INTEGER NOT NULL,
    "NAME" NVARCHAR(50),
    "QUALIFICATION" NVARCHAR(20),
    "WORK_EXP" NVARCHAR(10),
    "YRS_IN_COMP" NVARCHAR(10),
    "DESIGNATION" NVARCHAR(20),
    "ROLE" NVARCHAR(20),
    PRIMARY KEY("REQUEST_NO", "SR_NO")
)
TYPE "ST_PPR_ATTACHMENT" AS TABLE(
  "PPR_NO" BIGINT,
  "ATTACH_CODE" INTEGER,
  "FORM_ID" INTEGER,
  "FILE_ID" INTEGER,
  "FILE_NAME" NVARCHAR(100),
  "FILE_TYPE" NVARCHAR(100),
  "FILE_MIMETYPE" NVARCHAR(100),
  "FILE_CONTENT" BLOB,
  "UPLOAD_DATE" TIMESTAMP,
  PRIMARY KEY("PPR_NO", "FORM_ID", "FILE_ID")
)
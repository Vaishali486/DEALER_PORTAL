TYPE "ST_RETAILER_PDC" AS TABLE(
  
    "DISTRIBUTOR_ID" NVARCHAR(10),
    "RETAILER_ID" NVARCHAR(10),
    "PDC_ID" INTEGER,
    "NAME_OF_BANK" NVARCHAR(20),
    "CHEQUE_NUMBER" INTEGER,
    "CREATION_DATE" DATE,
    "AMOUNT" DOUBLE,
    "CURR_CODE" NVARCHAR(5),
    
    PRIMARY KEY("DISTRIBUTOR_ID", "RETAILER_ID","PDC_ID")

)
TYPE "ST_PPR_EVENTS" AS TABLE(
  "PPR_NO" BIGINT,
  "EVENT_NO" INTEGER,
  "EVENT_CODE" INTEGER,
  "USER_NAME" NVARCHAR(100),
  "USER_ROLE" NVARCHAR(100),
  "USER_ID" NVARCHAR(100),
  "REMARK" NVARCHAR(100),
  "COMMENT" NVARCHAR(200),
  "CREATION_DATE" TIMESTAMP,
  PRIMARY KEY("PPR_NO", "EVENT_NO")
) 
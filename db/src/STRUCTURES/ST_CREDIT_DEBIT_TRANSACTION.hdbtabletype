TYPE "ST_CREDIT_DEBIT_TRANSACTION" as TABLE (

  DISTRIBUTOR_ID NVARCHAR(10) NOT NULL,
  RETAILER_ID NVARCHAR(10) NOT NULL,
  TX_ID BIGINT NOT NULL,
  TRANSACTION_TYPE NVARCHAR(20),
  POSTING_DATE DATE,
  REFERENCE_INVOICE NVARCHAR(15),
  INVOICE_AMOUNT NVARCHAR(20),
  REFERENCE_TEXT NVARCHAR(200),
  AMOUNT NVARCHAR(20),
  TAX_AMOUNT NVARCHAR(20),
  TOTAL_AMOUNT NVARCHAR(20),
  CREATED_ON TIMESTAMP,
  PRIMARY KEY(DISTRIBUTOR_ID, RETAILER_ID, TX_ID)
)
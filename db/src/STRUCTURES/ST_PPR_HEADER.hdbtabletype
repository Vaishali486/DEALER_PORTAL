TYPE "ST_PPR_HEADER" AS TABLE(
  "PPR_NO" BIGINT,
  "PROD_GRP" NVARCHAR(100),
  "DISTRIBUTOR_ID" NVARCHAR(100),
  "DISTRIBUTOR_NAME" NVARCHAR(100),
  "PROD_CODE" NVARCHAR(100),
  "PROD_UNKNOWN" NVARCHAR(1),
  "FACTORY_NAME" NVARCHAR(100),
  "DESCRIPTION" NVARCHAR(100),
  "STATUS" INTEGER,
  "SALES_ASSOCIATE_ID" NVARCHAR(100),
  "APPROVER_ROLE" NVARCHAR(100),
  "APPROVER_LEVEL" INTEGER,
  "CREATED_ON" TIMESTAMP,
  PRIMARY KEY("PPR_NO", "PROD_GRP", "DISTRIBUTOR_ID", "FACTORY_NAME")
)
